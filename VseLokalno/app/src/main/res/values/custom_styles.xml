<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="RoundedSquare">
        <item name="cornerSizeBottomLeft">10dp</item>
        <item name="cornerSizeBottomRight">10dp</item>
    </style>

    <style name="FullRoundedSquare">
        <item name="cornerSizeBottomLeft">10dp</item>
        <item name="cornerSizeBottomRight">10dp</item>
        <item name="cornerSizeTopLeft">10dp</item>
        <item name="cornerSizeTopRight">10dp</item>
    </style>

    <style name="ExtraRoundedSquare">
        <item name="cornerSizeBottomLeft">20dp</item>
        <item name="cornerSizeBottomRight">20dp</item>
        <item name="cornerSizeTopLeft">20dp</item>
        <item name="cornerSizeTopRight">20dp</item>
    </style>

    <style name="RatingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">@color/blue_normal</item>
        <item name="colorControlActivated">@color/red_normal</item>
    </style>

    <style name="FullCircle">
        <item name="cornerSizeBottomLeft">100dp</item>
        <item name="cornerSizeBottomRight">100dp</item>
        <item name="cornerSizeTopLeft">100dp</item>
        <item name="cornerSizeTopRight">100dp</item>
    </style>

</resources>